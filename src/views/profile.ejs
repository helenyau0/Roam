<% include ./header%>
In user's profile page
  <h1 class="whatever"><%= user.name%></h1>
  <h1><%= user.current_city%></h1>
  <h1><%= user.created_at%></h1>
  <button type="button" class="btn btn-primary editButton"  data-toggle="modal" data-target="#exampleModal">
    Launch demo modal
  </button>
  <form class="" action="/users/update" method="post">
    <!-- Modal -->
    <div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
      <div class="modal-dialog" role="document">
        <div class="modal-content">
          <div class="modal-header">
            <h5 class="modal-title" id="exampleModalLabel">Modal title</h5>
            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
              <span aria-hidden="true">&times;</span>
            </button>
          </div>
          <div class="modal-body">
            <input type="text" name="name" value=user.name>
            <input type="text" name="current_city" value="user.current_city">
          </div>
          <div class="modal-footer">
            <button type="button" class="btn btn-secondary exit" data-dismiss="modal">Close</button>
            <button type="submit" class="btn btn-primary">Save changes</button>
          </div>
        </div>
      </div>
    </div>
  </form>
  <script type="text/javascript" src="index.js"></script>
