<!DOCTYPE html>
<nav class="navbar navbar-inverse bg-inverse">
  <h1 class="logo">
    <a class="left" href="/">Roam</a>
  </h1>
  <h1 class="nav-city-name">
    <% if (city) { %>
      <%= city.name %>
    <% } %>
  </h1>
  <% if (userSess) { %>
    <a class="right" href="/logout">Logout</a>
    <a class="right" href="/users/<%=userSess.name%>">Profile</a>
  <% } else { %>
    <a class="right" href="/login">Login</a>
    <a class="right" href="/users/signup">Sign Up</a>
  <% } %>
</nav>
