<!DOCTYPE html>
<% include ./partials/header%>

<h3>Pages: 
  <% for (let i = 1; i <= posts.length ; i++) { %>
    <a href='/cities/<%=city.name%>?pages=<%= i %>'><%= i %></a>
  <% } %>
</h3>

<div class="container city-page-content">
    
  <div class="right">
    <% if(userSess) {%>
      <button type="button" class="btn btn-primary editButton"  data-toggle="modal" data-target="#exampleModal">Add New Post</button>
      <% include ./modals/posts_add%>
    <%}%>
  </div>

  <div class="left">
    <% include ./lists/posts%>
  </div>

</div>


<script src='/index.js'></script>
